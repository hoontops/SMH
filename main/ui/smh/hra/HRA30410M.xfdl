this.fn_cancle = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var clearcom = '<fc v="red">*</fc>답변내용\n(0/ 1000)' ;

	this.div_work.Tabpage2.form.div_basicInfo.form.txa_answer.set_value("");  //답변부분 초기화
