			//objDs.filter(strFilter);	
			break;
		case "NEQ":	//Does Not Equal
			strFilter = colNm + "!=" +  this.gfn_quote(this.tab.Tabpage2.form.div_custom.form.cbo_values.value); 
			//objDs.filter(strFilter);	
			break;
		default:
	}
