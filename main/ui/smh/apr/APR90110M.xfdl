 * 기타 함수 : 조회조건 초기값 설정
*/
this.fn_formInit = function ()
{	
	this.ds_search.setColumn(0, "APPRAISAL_YEAR", this.gfn_getYear()); //Sysdate 기준년도
    this.ds_search.setColumn(0, "APPRAISAL_SEQUENCE", "1"); //1st
};

