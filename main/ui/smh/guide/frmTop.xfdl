/* 메뉴로그쌓기 */
this.fn_insertJobMenuLogs = function (menuId)
{
	var sSvcID = "insertJobMenuLogs";
	var sController = "/cmlm01100/insertJobMenuLogs.do";
	var sArgs = "";
