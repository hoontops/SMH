	this.gfn_transaction(sSvcID, sController, sInDatasets, sOutDatasets, sArgs, sCallbackFunc);
};

/* 즐겨찾기 메뉴서비스 */
this.fn_getMyMenu = function ()
{
	var sSvcID = "selectMyMenu";
	var sController = "/penc00010/selectFavorites.do";
