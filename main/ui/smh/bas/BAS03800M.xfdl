};

this.tab_search_Tabpage1_edt_durableCode_onkeydown = function(obj:nexacro.Edit,e:nexacro.KeyEventInfo)
{
	if (e.keycode != 13) return;
	
	this.ds_search.setColumn(0,"DURABLEID",obj.value);
