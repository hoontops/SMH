	var enterpriseId	= this.searchDiv.cmb_enterpriseid.value;
	var title			= this.nfn_nvl(this.nfn_getDictionarynameUpper("PRINTSPECCARD"),"이력카드");
	
	var oArg = {
		sRptPath   : "/bas/",
		sRptName   : "BAS02000R.mrd",
