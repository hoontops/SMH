 ***************************************************************************/

this.grd_cmdUsers_onselectchanged = function (obj:Grid, e:nexacro.GridSelectEventInfo)
{
	this.fn_search2();
};


this.div_search_btn_clear1_onclick = function (obj:Button, e:nexacro.ClickEventInfo)
{
	this.reload();
};
