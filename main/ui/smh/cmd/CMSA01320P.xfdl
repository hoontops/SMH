this.fn_callBack = function (trId, errCD, errMsg)
{
	if (errCD < 0) 
	{
		if (trId.substring(0, 6) == "select") 
		{
			alert("조회에 실패하였습니다." + errMsg + ".." + trId);
		}
		else 
		{
			alert(errMsg);
		}
	}
};


