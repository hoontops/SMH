            this.fn_searchYieldRateByLOTList();      //탭1 LOT별 수율
        break;
        case 1:
            this.fn_searchDefectStatusByLOTList();   //탭2 LOT별 불량현황
        break;
        case 2:
            this.fn_searchDefectDetailByLOTList();   //탭3 불량세부
        break;
    }
};

