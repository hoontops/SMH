    if (sPopupId == "SRCH_PRODUCTDEF280")    /*조회조건-품목*/
    {
		var productName = "";
		var productIdVer= "";
		
		for(var i=0; i<rtn.length; i++){
			var colArray = rtn[i];	
			if (i > 0)
			{
				productName  += ",";
				productIdVer += ",";
			}			
			productName  += colArray[0];
			productIdVer += colArray[1];
		}	
		
