{
	if (errCD < 0) 
	{
		this.gfn_Message(errMsg, "", "error", "ok");
		//this.gfn_Message("ProcessingError", trId + errMsg, "error", "ok");
	}
	else 
	{
		if (trId == "saveCloseProcess") 
		{
			this.gfn_Message("ProcessingSuccess", null, "info", "ok");
			this.fn_search();
		}
		else if (trId == "saveCloseCancel") 
		{
			this.gfn_Message("ProcessingSuccess", null, "info", "ok");
			this.fn_search();
		}		
	}

}


