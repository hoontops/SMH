{	
	//현재탭 위치 저장 플래그
	tab = e.postindex;
	
	// dataset 현재 row
	var selRow = this.ds_adjLifeExpenseList.rowposition
	
	trace("===selRow : "+ selRow);
	
	// 선택 row 신청 상태 가져오기
