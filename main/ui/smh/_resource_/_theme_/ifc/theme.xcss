{
	-nexa-border : 1px solid #f1f1f1;
	-nexa-padding : 1px 3px;
	-nexa-text-align : center;
	font : normal 12px/normal "Nanum Gothic", "나눔 고딕","Arial";
	background : #ffffff;
	color : #555555;
}

.Grid .body .row .cell .subcell[status=mouseover]
{
	background : #f0f0f0;
}

.Grid .body .row .cell .subcell[userstatus=selected]
{
	background : #daf1fd;
	color : #000000;
}

.Grid .body .row .cell .subcell[status=disabled]
{
	color : #888888;
}
