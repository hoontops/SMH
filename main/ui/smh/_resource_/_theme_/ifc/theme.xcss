/**********************Button***********************************************/
/**********************FileDownload***********************************************/
/**********************FileUpload***********************************************/
.Button,.Button[userstatus=selected]
{
	background : #ffffff;
	-nexa-border : 0px none;
	cursor : pointer;
	color : #333333;
	-nexa-padding : 0px 10px;
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
	font : normal 400 12px/normal "Nanum Gothic", "나눔 고딕","Arial";
	-nexa-edge : url('theme://images/bg_btn_N.png') 4px 4px;
}

.ButtonControl
{
	background : transparent;
	-nexa-border : 0px none;
	cursor : pointer;
}

.Button[status=mouseover]
{
	-nexa-edge : url('theme://images/bg_btn_O.png') 4px 4px;
}

.Button[status=focused]
{
	-nexa-edge : url('theme://images/bg_btn_O.png') 4px 4px;
}

.Button[userstatus=pushed]
{
	-nexa-edge : url('theme://images/bg_btn_P.png') 4px 4px;
}

.Button[status=disabled]
{
	cursor : default;
	color : #888888;
	-nexa-edge : url('theme://images/bg_btn_D.png') 4px 4px;
}

.FileUpload
{
	/*별도스타일없음*/
}

.FileUpload .fileuploaditem
{
	/*별도스타일없음*/
}

.FileUpload .fileuploaditem[status=disabled]
{
	color : #888888;
}
.FileUpload .fileuploaditem .fileuploaditembutton,
.FileUpload .fileuploaditem .fileuploaditembutton[userstatus=selected]
{
	background : #999999;
	-nexa-border : 0px none,0px none,0px none,3px solid #ffffff;
	color : #ffffff;
	-nexa-padding : 0px 10px;
}
.FileUpload .fileuploaditem .fileuploaditembutton[status=mouseover]
{
	background : #444444;
	color : #ffffff;
	-nexa-border : 0px none,0px none,0px none,3px solid #ffffff;
}
.FileUpload .fileuploaditem .fileuploaditembutton[status=focused]
{
	background : #444444;
	color : #ffffff;
	-nexa-border : 0px none,0px none,0px none,3px solid #ffffff;
	box-shadow : 0px 0px 2px #8daee5;
}
.FileUpload .fileuploaditem .fileuploaditembutton[userstatus=pushed]
{
	background : #000000;
	color : #ffffff;
	-nexa-border : 0px none,0px none,0px none,3px solid #ffffff;
}

.FileUpload .fileuploaditem .fileuploaditembutton[status=disabled]
{
	opacity : 1;
	background : #c5c5c5;
}

.FileDownload
{
	background : #999999;
	-nexa-border : 0px none;
	color : #ffffff;
	-nexa-padding : 0px 10px;
}

.FileDownload[status=mouseover]
{
	background : #444444;
	-nexa-border : 0px none;
	color : #ffffff;
}

.FileDownload[status=focused]
{
	background : #444444;
	-nexa-border : 0px none;
	color : #ffffff;
	box-shadow : 0px 0px 2px #8daee5;
}

.FileDownload[userstatus=pushed]
{
	background : #000000;
	-nexa-border : 0px none;
	color : #ffffff;
}

.FileDownload[status=disabled]
{
	opacity : 1;
	background : #c5c5c5;
}

