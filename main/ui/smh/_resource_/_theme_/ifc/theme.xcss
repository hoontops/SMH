	-nexa-border : 1px solid #1476c7,0px none,0px none;
	background : #ffffff;
}

.Tab.tab_master_02 .tabbuttonitem
{
	background : transparent;
	border-radius : 7px 7px 0px 0px;
	-nexa-border : 1px solid transparent;
	-nexa-padding : 6px 20px 6px 20px;
	color : #777777;
	font : normal 700 13px/normal "Nanum Gothic", "나눔 고딕","Arial";
}

.Tab.tab_master_02 .tabbuttonitem[status=mouseover]
{
	color : #000000;
}

.Tab.tab_master_02 .tabbuttonitem[status=focused]
{
	color : #000000;
}

.Tab.tab_master_02 .tabbuttonitem[userstatus=selected]
{
	background : #ffffff;
	border-radius : 7px 7px 0px 0px;
	color : #1476c7;
	-nexa-border : 1px solid #1476c7,1px solid #1476c7,0px none,1px solid #1476c7;
	-nexa-padding : 6px 20px 6px 20px;	
}

.Tab.tab_master_02 .tabbuttonitem[status=disabled]
{
	color : #aaafb4;
	background : #ffffff;
	-nexa-border : 0px none,0px none,1px solid #1476c7,0px none;
}

.Tab.tab_master_02 .tabbuttonitem .tabbuttonitemtext
{
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
}

.Tab.tab_master_02 .tabbuttonitem .tabbuttonitemtext[status=mouseover]
{
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
	-nexa-text-decoration : underline;
}

.Tab.tab_master_02 .tabbuttonitem .tabbuttonitemtext[status=disabled]
{
	color : #888888;
	-nexa-text-align : center;
	-nexa-vertical-align : middle;
}

.Tab .tabbuttonitem.tab_btn_op
{
 opacity : 0;
}]]></XCSS>
