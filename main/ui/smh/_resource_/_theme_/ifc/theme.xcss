/********************** Frame/Form ***************************************/
.Form
{
	font : normal 12px/normal "Nanum Gothic", "나눔 고딕","Arial";
	color : #333333;
	background : #ffffff;
	-nexa-border : 0px none;
}

.Form[userstatus=contents]
{
	background : transparent;
}

.Form[status=disabled]
{
	color : #888888;
}

.MainFrame,.ChildFrame,.FrameSet,.VFrameSet,.HFrameSet,.TileFrameSet
{
	/* No style applied */
}

