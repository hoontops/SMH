this.gridDiv = this.div_work.form;

this.productId = ''; //제품Id(조회조건)
this.productVer = ''; //제품Ver(조회조건)

this.productKeyType = '1'; //1:조회조건, 2:그리드

this.fv_rtnValue = ''; //팝업화면 리턴값 변수

/**************************************************************************
 * 3. FORM LOAD
 ***************************************************************************/
this.fn_init = function (obj:Form, e:nexacro.LoadEventInfo)
{
