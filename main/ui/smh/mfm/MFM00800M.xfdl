this.div_work_grd_priority_onexpandup = function(obj:nexacro.Grid,e:nexacro.GridMouseEventInfo)
{
	if(e.row < 0) return;
	if (this.nfn_isEqualCol(obj, e.col, "PRODUCTDEFID")) {	
		this.fn_productPop();
