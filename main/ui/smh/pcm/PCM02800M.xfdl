	var newvalue = this.nfn_nvl(e.newvalue , "");
	
	if(e.columnid == "P_AREANM")
	{
		if(newvalue.trim() == "")
		{
			this.g_areaId = "";
			this.ds_search.setColumn( 0, "P_AREAID",   "" );	
		}
		else if(oldvalue.trim() != newvalue.trim())
