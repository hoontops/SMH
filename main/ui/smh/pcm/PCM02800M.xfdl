			oArg.arg_popupCd     = "P00156";
			oArg.arg_popupNm     = "공정 선택";
			oArg.arg_rtnCols     = "PROCESSSEGMENTID|PROCESSSEGMENTNAME";
			oArg.arg_paramCols   = "P_POPTYPE|P_LANGUAGETYPE";
