	this.ds_search.setColumn(0, "PRODUCTDEFID", "");
	this.ds_search.setColumn(0, "AREANAME", "");
	this.ds_search.setColumn(0, "PROCESSSEGMENTNAME", "");	
	this.div_work.form.grd_main.setFormatColProperty(4, "size", 50);
