									  + "[" +  areaId +  "]"
									  + "[" +  plantId +  "]"
									  + "[" +  lotType +  "]"
									  + "[" +  productId +  "]"
									  + "[" +  productRev +  "]",
						bModalless : false
						};
					
					var sOption = "titletext=공정별 재공실사 LIST ,modeless=false";
					this.gfn_reportPopup("reportViewer", "common::com_report.xfdl", oArg, sOption);
	}
};
