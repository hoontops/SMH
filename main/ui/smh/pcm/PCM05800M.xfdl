		var lotType = this.ds_search.getColumn(0, "PRODUCTIONTYPE"); //$5
		var productId = this.ds_search.getColumn(0, "PRODUCTDEFID"); //$6
		var productRev = this.ds_search.getColumn(0, "PRODUCTDEFVERSION"); //$7
