this.fn_search = function (obj:Button, e:nexacro.ClickEventInfo)
{
	var component = this.tab_search.Tabpage1;
// 	var strColIdList = "cbo_plantid,cbo_LOTPRODUCTTYPESTATUS,cbo_PRODUCTDEFTYPE";
// 	var strColNmList = "SITE,LOTPRODUCTTYPE,PRODUCTDEFTYPE";
