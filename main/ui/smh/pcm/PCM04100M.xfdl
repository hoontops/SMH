/*
 * 기능 : 저장(승인)
 */
this.fn_save = function (obj:Button, e:nexacro.ClickEventInfo)
{
	if(this.ds_pcmLothold.rowcount==0){
		this.gfn_Message("NoSaveData", null, "info", "ok");
		return;
	}
	var component = this.div_work.form.div_reason;
	var strColIdList = "cbo_reasoncodeclassid,cbo_reasoncodeid";
	var strColNmList = "LARGECLASS,MIDDLECLASS";
	var rtn = this.nfn_MandatoryCheck(component, strColIdList, strColNmList);
	if (rtn == false) return;
	
	for(var i=0; i<this.ds_pcmLothold.rowcount;i++){
		this.ds_pcmLothold.setColumn(i,"HOLDTOPCLASSID",this.div_work.form.div_reason.form.cbo_reasoncodeclassid.value);
		this.ds_pcmLothold.setColumn(i,"HOLDMIDDLECLASSID",this.div_work.form.div_reason.form.cbo_reasoncodeid.value);
		this.ds_pcmLothold.setColumn(i,"HOLDCOMMENT",this.div_work.form.div_reason.form.txa_holdcomment.value);
	}
	// -------------------------
	// 저장
	// ':U' 는 갱신된 내용만을 입력 데이터세트로 보내며, ':A' 는 모든 정보를 보냅니다.
	// ':N' 은 현재 Delete를 제외한 데이터를 Normal 상태로 보냅니다. 지정하지 않는 데이터세트는 Normal로 동작합니다
	// -------------------------
	var sSvcID = "savePcmLothold";
	var sController = "/pcm04100/savePcmLothold.do";
	var sInDatasets = "INPUT=ds_pcmLothold:U";
	var sOutDatasets = "";

	var sArgs = "";
	this.gfn_transaction(sSvcID, sController, sInDatasets, sOutDatasets, sArgs);
};

